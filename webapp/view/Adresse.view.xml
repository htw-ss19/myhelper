<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="myhelper.myhelper.controller.Adresse">
	<App>
		<pages>
			<Page title="Title">
				<content>
					<Bar id="bar1">
						<contentRight>
							<ComboBox xmlns="sap.m" id="box0" placeholder="Mein Benutzer">
								<items>
									<core:Item xmlns:core="sap.ui.core" key="item-1-key" text="Mein Profil" id="item2"/>
									<core:Item xmlns:core="sap.ui.core" key="item-2-key" text="Meine Profileinstellungen" id="item3"/>
									<core:Item xmlns:core="sap.ui.core" key="item-3-key" text="Meine Bestellungen" id="item4"/>
									<core:Item xmlns:core="sap.ui.core" key="item-4-key" text="Abmelden" id="item5"/>
								</items>
							</ComboBox>
						</contentRight>
						<contentLeft>
							<Image xmlns="sap.m" src="https://ui5.sap.com/resources/sap/ui/documentation/sdk/images/logo_ui5.png" width="50px" height="50px" id="image0"
								press="action" xmlns:action="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
								action:wiring="\{'press':\{'navigation':\{'routeName':'Startseite'\}\}\}"/>
						</contentLeft>
					</Bar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>